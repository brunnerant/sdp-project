<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/questionFrame"
        android:name="ch.epfl.qedit.view.question.QuestionFragment"
        android:layout_width="764dp"
        android:layout_height="match_parent"
        android:layout_gravity="right">


    </FrameLayout>

    <FrameLayout
        android:id="@+id/quizOverviewFrame"
        android:name="ch.epfl.qedit.view.question.QuizOverviewFragment"
        android:layout_width="200dp"
        android:layout_height="match_parent"
        android:layout_gravity="left">

    </FrameLayout>

</FrameLayout>
